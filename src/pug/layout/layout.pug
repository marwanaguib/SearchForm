doctype html
html(lang="en-US")
	head
		meta(charset="utf-8")
		meta(name="viewport", content="width=device-width, initial-scale=1, shrink-to-fit=no")
		block vars
		-var pageURL = pageURL ? pageURL : ''
		title Forms - #{title}
		meta(name="description",content=description)
	body(class=pageClass ? pageClass : '', id=pageId ? pageId : '')
		.pageContent
			block content
